{
  "expo": {
    "name": "BeatIt",
    "slug": "rear",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rear",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "reactNativeTrackPlayer": {
      "playbackService": "./services/trackPlayerService.ts"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeName": "Audio",
            "LSItemContentTypes": [
              "public.audio"
            ],
            "CFBundleTypeRole": "Viewer"
          },
          {
            "CFBundleTypeName": "Video",
            "LSItemContentTypes": [
              "public.video"
            ],
            "CFBundleTypeRole": "Viewer"
          }
        ]
      }
    },
    "widgets": [
      {
        "name": "MusicPlayerWidget",
        "bundleIdentifier": "com.anonymous.beatit.widget",
        "widgetFamily": [
          "systemSmall",
          "systemMedium"
        ],
        "widgetBackgroundColor": "#222",
        "widgetDisplayName": "Now Playing"
      }
    ],
    "extra": {
      "eas": {
        "projectId": "1655e879-0c32-4f6a-8eb7-8692cddb668d"
      }
    },
    "owner": "suman334",
    "android": {
      "package": "com.suman334.rear"
    },
    "plugins": [
      "expo-video"
    ]
  },
  "edgeToEdgeEnabled": true,
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/images/adaptive-icon.png",
      "backgroundColor": "#000000"
    },
    "edgeToEdgeEnabled": true,
    "package": "com.anonymous.beatit",
    "permissions": [
      "INTERNET",
      "android.permission.WRITE_EXTERNAL_STORAGE",
      "android.permission.READ_EXTERNAL_STORAGE",
      "android.permission.RECORD_AUDIO",
      "android.permission.MODIFY_AUDIO_SETTINGS",
      "android.permission.POST_NOTIFICATIONS",
      "WAKE_LOCK",
      "FOREGROUND_SERVICE",
      "android.permission.NFC",
      "FOREGROUND_SERVICE"
    ],
    "intentFilters": [
      {
        "action": "VIEW",
        "data": [
          {
            "scheme": "content",
            "mimeType": "audio/*"
          },
          {
            "scheme": "file",
            "mimeType": "audio/*"
          },
          {
            "scheme": "content",
            "mimeType": "video/*"
          },
          {
            "scheme": "file",
            "mimeType": "video/*"
          }
        ],
        "categories": [
          "BROWSABLE",
          "DEFAULT"
        ]
      }
    ]
  },
  "widgets": [
    {
      "name": "MusicPlayerWidget",
      "provider": ".MusicPlayerWidgetProvider",
      "minWidth": 250,
      "minHeight": 100,
      "updatePeriodMillis": 1000
    }
  ],
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./assets/images/favicon.png"
  },
  "plugins": [
    "expo-router",
    [
      "expo-splash-screen",
      {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      }
    ],
    [
      "expo-video",
      {
        "supportsBackgroundPlayback": true,
        "supportsPictureInPicture": true
      }
    ],
    [
      "expo-notifications",
      {
        "color": "#ffffff",
        "sounds": [],
        "mode": "production"
      }
    ],
    [
      "expo-media-library",
      {
        "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to share song artwork.",
        "savePhotosPermission": "Allow $(PRODUCT_NAME) to save song artwork to your photo library.",
        "isAccessMediaLocationEnabled": true
      }
    ],
    "expo-web-browser",
    [
      "@react-native-google-signin/google-signin",
      {
        "iosUrlScheme": "com.googleusercontent.apps.903522270495-t5vjfcpsmbdh08sc543ri9m12nsnt3ni"
      }
    ],
    "expo-audio",
    "expo-speech-recognition"
  ],
  "experiments": {
    "typedRoutes": true
  },
  "extra": {
    "router": {},
    "eas": {
      "projectId": "1655e879-0c32-4f6a-8eb7-8692cddb668d"
    }
  }
}
